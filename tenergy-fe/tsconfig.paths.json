{
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@pages": [
                "./pages"
            ],
            "@component": [
                "./components"
            ],
            "@component/*": [
                "./components/*"
            ],
            "@container": [
                "./containers"
            ],
            "@container/*": [
                "./containers/*"
            ],
            "@assets": [
                "./assets"
            ],
            "@styles": [
                "./styles"
            ],
            "@styles/*": [
                "./styles/*"
            ],
            "@store/*": [
                "./store/*"
            ],
            "@hoc": [
                "./hoc"
            ],
            "@api": [
                "./api"
            ],
            "@hook": [
                "./hooks"
            ]
        }
    }
}